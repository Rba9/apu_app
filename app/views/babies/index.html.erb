<p id="notice"><%= notice %></p>

<h1>Babies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Volumen</th>
      <th>Tiempo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @babies.each do |baby| %>
      <tr>
        <td><%= baby.name %></td>
        <td><%= baby.total_volumen %></td>
        <td><%= baby.total_time %></td>
      </tr>

  </tbody>
</table>
<br>
<%= link_to 'Show', baby %></td>
<%= link_to 'Edit', edit_baby_path(baby) %>
<%= link_to 'Destroy', baby, method: :delete, data: { confirm: 'Are you sure?' } %>
  <% end %>
<br>
<%= link_to 'New Baby', new_baby_path %>
